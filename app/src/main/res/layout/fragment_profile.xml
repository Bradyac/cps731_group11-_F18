<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/display_fname"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="25dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/display_lname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="34dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/display_email"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/display_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="76dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/trans_1_type"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/trans_1_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/trans_1_type"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/trans_1_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/trans_1_button"
        app:layout_constraintStart_toEndOf="@+id/trans_1_desc" />

    <Button
        android:id="@+id/trans_1_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/trans_1_type"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/trans_2_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/trans_2_type"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/trans_2_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/trans_2_button"
        app:layout_constraintStart_toEndOf="@+id/trans_2_desc" />

    <TextView
        android:id="@+id/trans_3_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/trans_3_type"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/trans_3_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="45dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/trans_3_desc"
        app:layout_constraintTop_toBottomOf="@+id/trans_2_type" />

    <Button
        android:id="@+id/trans_2_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:text="Button"
        app:layout_constraintStart_toStartOf="@+id/trans_1_button"
        app:layout_constraintTop_toBottomOf="@+id/trans_1_button" />

    <Button
        android:id="@+id/trans_3_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/trans_3_type"
        app:layout_constraintStart_toStartOf="@+id/trans_2_button" />

    <TextView
        android:id="@+id/trans_1_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="1dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/trans_1_button"
        app:layout_constraintEnd_toStartOf="@+id/trans_1_button" />

    <TextView
        android:id="@+id/trans_2_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/trans_2_button"
        app:layout_constraintEnd_toStartOf="@+id/trans_2_button" />

    <TextView
        android:id="@+id/trans_3_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/trans_3_button"
        app:layout_constraintEnd_toStartOf="@+id/trans_3_button" />

</android.support.constraint.ConstraintLayout>